@page "/"
@page "/counter"

<PageTitle>Counter</PageTitle>

<h1>Counter</h1>

<p role="status">Redux current count: @Container.State.Counter</p>
<p role="status">DataService current count: @DataService.Counter.Value</p>

<button class="btn btn-primary" @onclick="btnIncrement_Click">Add</button>
<button class="btn btn-primary" @onclick="btnDecrement_Click">Remove</button>